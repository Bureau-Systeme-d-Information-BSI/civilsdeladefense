var node = document.querySelector('.job-application-files')
if (node !== null) {
  var html = '<%= j render(template: '/admin/job_applications/files') %>'
  var frag = document.createRange().createContextualFragment(html)
  node.replaceWith(frag)
}
Snackbar.show({showAction: false, text: '<%= @notification %>'})
formAutoSubmit()
