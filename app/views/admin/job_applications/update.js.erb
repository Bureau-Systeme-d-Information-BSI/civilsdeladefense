var dom_id = '#<%= dom_id(@job_application, 'profile') %>';
var node = document.querySelector(dom_id)
if (node !== null) {
  var html = '<%= j render(partial: '/admin/job_applications/job_application_profile', locals: { job_application: @job_application }) %>'
  var frag = document.createRange().createContextualFragment(html)
  node.replaceWith(frag)
}
$.snackbar({content: '<%= @notification %>'})
formAutoSubmit()
