var dom_id = '#<%= dom_id(@user, 'profile') %>';
var node = document.querySelector(dom_id)
if (node !== null) {
  var html = '<%= j render(partial: '/admin/profiles/personal_profile', locals: { user: @user }) %>'
  var frag = document.createRange().createContextualFragment(html)
  node.replaceWith(frag)
}
Snackbar.show({showAction: false, text: '<%= @notification %>'})
formAutoSubmit()
