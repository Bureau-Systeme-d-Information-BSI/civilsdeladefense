<%- if current_organization.matomo_site_id.present? %>
<script type="text/javascript">
  var _paq = _paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="//stats.data.gouv.fr/";
    _paq.push(['setTrackerUrl', u+'piwik.php']);
    _paq.push(['setSiteId', '<%= current_organization.matomo_site_id %>']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
<%- end %>
<%- if current_organization.atinternet_site_id.present? %>
  <script type="text/javascript" src="https://tag.aticdn.net/<%= current_organization.atinternet_site_id %>/smarttag.js"></script>
  <script type="text/javascript">
    var ATTag = new ATInternet.Tracker.Tag();
    ATTag.page.send({name: '<%= @page_title || "CVD" %>' });
  </script>
<%- end %>
